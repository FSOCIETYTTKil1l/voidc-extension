func main() {
    print("Counting to 5:");
    int i = 1;
    while (i <= 5) {
        print(i);
        i = i + 1;
    }
    
    print("Countdown from 10:");
    int countdown = 10;
    while (countdown >= 0) {
        print(countdown);
        countdown = countdown - 1;
    }
    
    print("Even numbers up to 20:");
    int num = 0;
    while (num <= 20) {
        print(num);
        num = num + 2;
    }
}