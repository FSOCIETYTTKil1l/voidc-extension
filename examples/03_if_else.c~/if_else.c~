func main() {
    int age = 18;
    
    print("Checking age...");
    
    if (age >= 18) {
        print("You are an adult!");
    } else {
        print("You are a minor!");
    }
    
    int score = 85;
    
    if (score >= 90) {
        print("Grade: A");
    } else {
        if (score >= 80) {
            print("Grade: B");
        } else {
            if (score >= 70) {
                print("Grade: C");
            } else {
                print("Grade: F");
            }
        }
    }
}