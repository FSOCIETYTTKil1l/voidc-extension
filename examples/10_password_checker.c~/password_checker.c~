func checkPasswordLength(int length) {
    if (length < 8) {
        print("WEAK: Password too short!");
        return 0;
    }
    if (length >= 12) {
        print("STRONG: Good password length!");
        return 2;
    }
    print("MEDIUM: Password length acceptable");
    return 1;
}

func main() {
    print("=== Password Strength Checker ===");
    
    int passwordLength = 6;
    print("Testing password with length: ");
    print(passwordLength);
    checkPasswordLength(passwordLength);
    
    print("");
    passwordLength = 10;
    print("Testing password with length: ");
    print(passwordLength);
    checkPasswordLength(passwordLength);
    
    print("");
    passwordLength = 15;
    print("Testing password with length: ");
    print(passwordLength);
    checkPasswordLength(passwordLength);
}