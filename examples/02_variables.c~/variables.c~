func main() {
    int a = 10;
    int b = 20;
    int sum = a + b;
    int diff = b - a;
    int product = a * b;
    
    print("a = ");
    print(a);
    print("b = ");
    print(b);
    print("Sum: ");
    print(sum);
    print("Difference: ");
    print(diff);
    print("Product: ");
    print(product);
}