func fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    
    int a = 0;
    int b = 1;
    int i = 2;
    
    while (i <= n) {
        int temp = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    
    return b;
}

func main() {
    print("Fibonacci Sequence:");
    
    int i = 0;
    while (i < 10) {
        int fib = fibonacci(i);
        print(fib);
        i = i + 1;
    }
}