func isPrime(int n) {
    if (n <= 1) {
        return 0;
    }
    
    if (n == 2) {
        return 1;
    }
    
    int i = 2;
    while (i * i <= n) {
        if (n % i == 0) {
            return 0;
        }
        i = i + 1;
    }
    
    return 1;
}

func main() {
    print("Prime numbers up to 50:");
    
    int num = 2;
    while (num <= 50) {
        if (isPrime(num) == 1) {
            print(num);
        }
        num = num + 1;
    }
}